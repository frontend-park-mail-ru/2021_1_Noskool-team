{
  "compilerOptions": {
    "outDir": "./out/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "jsxFactory": "JSX",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "jsx/*": [
        "jsx/*"
      ],
      "actions/*": [
        "actions/*"
      ],
      "pages/*": [
        "pages/*"
      ],
      "constants/*": [
        "constants/*"
      ],
      "modules/*": [
        "modules/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "types/*": [
        "types/*"
      ]
    }
  },
}
