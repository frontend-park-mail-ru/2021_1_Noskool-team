{
  "extends": [
    "stylelint-config-standard"
  ],
  "rules": {
    "at-rule-no-unknown": [true, {
      "ignoreAtRules": ["include", "mixin"]
    }],
    "font-family-no-missing-generic-family-keyword": [
      true,
      {
        "ignoreFontFamilies": [""]
      }
    ],
    "no-descending-specificity": null
  }
}
